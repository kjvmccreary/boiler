# Since Vite proxy handles /api -> http://localhost:5000, 
# we don't need a full base URL
VITE_API_BASE_URL=

# Or alternatively, use relative path
# VITE_API_BASE_URL=/

# App Configuration
VITE_APP_NAME=Microservices Starter
VITE_APP_VERSION=1.0.0

# Debug settings
VITE_DEBUG=true

# MSW Configuration (set to 'true' to enable API mocking in development)
VITE_ENABLE_MOCK=false
