{
  "name": "microservices-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:production": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.js",
    "test:ui": "vitest --ui --config vitest.config.js",
    "test:coverage": "vitest run --coverage --config vitest.config.js",
    "test:watch": "vitest --watch --config vitest.config.js",
    "test:workflow:dsl": "vitest run src/features/workflow/tests/dsl --config vitest.config.js",
    "test:workflow:all": "vitest run src/features/workflow/tests --config vitest.config.js",
    "test:workflow:service": "vitest run src/features/workflow/tests/service --config vitest.config.js",
    "test:workflow:operations": "vitest run src/features/workflow/tests/operations --config vitest.config.js",
    "test:workflow:integration": "vitest run src/features/workflow/tests/integration-lite --config vitest.config.js",
    "test:workflow:builder": "vitest run src/features/workflow/tests/builder --config vitest.config.js",
    "test:workflow:coverage": "vitest run --coverage src/features/workflow/tests",
    "test:workflow:coverage:fast": "vitest run --coverage --config vitest.config.js src/features/workflow/tests/dsl src/features/workflow/tests/service src/features/workflow/tests/integration-lite src/features/workflow/tests/builder",
    "test:changed": "vitest --changed --config vitest.config.js",
    "test:ci": "vitest run --reporter=default --coverage --config vitest.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bundle-analyzer": "npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^3.3.0",
    "@microsoft/signalr": "^9.0.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts-pro": "^8.10.2",
    "@mui/x-data-grid-premium": "^8.10.2",
    "@mui/x-date-pickers-pro": "^8.10.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^24.2.0",
    "axios": "^1.11.0",
    "date-fns": "^2.30.0",
    "monaco-editor": "^0.53.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^7.7.1",
    "reactflow": "^11.11.4",
    "tailwindcss": "^4.1.11",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^23.0.0",
    "msw": "^2.10.4",
    "prettier": "^3.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^1.0.0"
  }
}
